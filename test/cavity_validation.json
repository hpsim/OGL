{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Product",
  "description": "",
  "type": "object",
  "patternProperties": {
    "[a-z0-9]+": {
      "type": "object",
      "properties": {
        "global": {
          "type": "string",
          "enum": [
            "completed"
          ]
        },
        "CourantNumber": {
          "type": "object",
          "properties": {
            "CourantNumber_max": {
              "type": "number",
              "exclusiveMinimum": 0,
              "Maximum": 1
            }
          }
        },
        "continuityErrors": {
          "type": "object",
          "properties": {
            "timeStepContErrors_sumLocal": {
              "type": "number",
              "exclusiveMaximum": 1e-08
            }
          }
        }
      }
    }
  },
  "minProperties": 1
}
