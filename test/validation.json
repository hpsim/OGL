{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Product",
  "description": "",
  "type": "object",
  "patternProperties": {
    "[a-z0-9]+": {
      "type": "object",
      "properties": {
        "global": {
          "type": "string",
          "enum": [
            "completed"
          ]
        },
        "CourantNumber": {
          "anyOf": [
            {
              "type": "object",
              "properties": {
                "CourantNumber_max": {
                  "type": "number",
                  "exclusiveMinimum": 0,
                  "Maximum": 1
                }
              }
            },
            {
              "type": "number",
              "const": 0.0
            }
          ]
        },
        "continuityErrors": {
          "type": "object",
          "properties": {
            "timeStepContErrors_sumLocal": {
              "type": "number",
              "exclusiveMaximum": 1e-07
            }
          }
        }
      }
    }
  },
  "minProperties": 1
}
